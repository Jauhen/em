{
    "name": "EPAM Memegen",
    "version": "2.0",
    "manifest_version": 2,
    "icons" : {
        "48" : "troll.jpeg",
        "128" : "troll.jpeg"
    },
    "description": "EPAM Memegen",
    "browser_action": {
        "default_icon": "troll.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
